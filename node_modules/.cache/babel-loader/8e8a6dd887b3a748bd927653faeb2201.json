  <slide>\n                    <div class=\"single-works carousel-green-hover\">\n                        <img src=\"../resources/photo16.jpeg\">\n                        <a v-if=\"windowWidth > 767\" @click=\"index = 1;$refs['modal-zoom'].show()\" style=\"cursor: pointer;\" class=\"icon\"><feather type=\"zoom-in\"></feather></a>\n\n                        <div class=\"works-content\">\n                            <h3><a href=\"#\">Profitability screening on autopilot</a></h3>\n                            <p>Automate manual search from Rightmove or Zoopla</p>\n                        </div>\n                    </div>\n                </slide>\n\n                <slide>\n                    <div class=\"single-works carousel-green-hover\">\n                        <img src=\"../resources/photo14.jpeg\" >\n                        <a v-if=\"windowWidth > 767\" @click=\"index = 2;$refs['modal-zoom'].show()\" style=\"cursor: pointer;\" class=\"icon\"><feather type=\"zoom-in\"></feather></a>\n\n                        <div class=\"works-content\">\n                            <h3><a href=\"#\">Investor friendly filters</a></h3>\n                            <p>Search properties by investor specific metrics</p>\n                        </div>\n                    </div>\n                </slide>\n\n                <slide>\n                    <div class=\"single-works carousel-green-hover\">\n                        <img src=\"../resources/photo15.jpeg\" >\n                        <a v-if=\"windowWidth > 767\" @click=\"index = 3;$refs['modal-zoom'].show()\" style=\"cursor: pointer;\" class=\"icon\"><feather type=\"zoom-in\"></feather></a>\n\n                        <div class=\"works-content\">\n                            <h3><a href=\"#\">Alert with top cash flow deals</a></h3>\n                            <p>Receive the highest cash flow opportunities by email</p>\n                        </div>\n                    </div>\n                </slide>\n            </carousel>\n        </div>\n        <div class=\"shape4\"><img src=\"../resources/shape4.svg\"></div>\n    </section>\n    <!-- End Works Area -->\n</template>\n\n<script>\nimport { Carousel, Slide } from 'vue-carousel';\n\nexport default {\n    name: 'WorksProperties',\n    components: { Carousel, Slide },\n    data() {\n        return {\n            modalZoom: ['modal-zoom'],\n            windowWidth: window.innerWidth,\n            index: 0,\n            modalProperties: [\n                {\n                   title : \"London residential listings in one place\",\n                   img: \"./photo13.jpeg\"\n                },\n                {\n                    title : \"Profitability screening on autopilot\",\n                    img: \"./photo16.jpeg\"\n                },\n                {\n                    title : \"Investor friendly filters\",\n                    img: \"./photo14.jpeg\"\n                },\n                {\n                    title : \"Alert with top cash flow deals\",\n                    img: \"./photo15.jpeg\"\n                }\n\n            ]\n        }\n    },\n    mounted() {\n        this.$nextTick(() => {\n            window.addEventListener('resize', this.onResize);\n        })\n    },\n\n    beforeDestroy() {\n        window.removeEventListener('resize', this.onResize);\n    },\n    methods: {\n        onResize() {\n            this.windowWidth = window.innerWidth\n        },\n        getImgUrl() {\n            var images = require.context('../resources/', false, /\\.jpeg$/)\n            return images(this.modalProperties[this.index].img)\n        }\n    }\n}\n</script>\n"],"sourceRoot":"src/views/ideas/properties/components"}]}