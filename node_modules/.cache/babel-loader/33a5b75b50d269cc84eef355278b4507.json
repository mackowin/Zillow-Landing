,CAFA;AAGA,IAAA,SAHA,uBAGA;AACA,WAAA,KAAA;AACA,WAAA,GAAA,GAAA,KAAA,WAAA,CAAA,OAAA,CAAA,GAAA;;AACA,UAAA,CAAA,KAAA,GAAA,EAAA;AACA,aAAA,GAAA,GAAA,QAAA;AACA;;AACA,UAAA,KAAA,WAAA,CAAA,OAAA,CAAA,KAAA,EAAA;AACA,aAAA,aAAA,GAAA,KAAA;AACA,aAAA,KAAA,CAAA,UAAA,CAAA,IAAA,CAAA;AACA,UAAA,GAAA,EAAA,KAAA,WAAA,CAAA,OAAA,CAAA;AADA,SAAA,EAEA,IAFA,CAEA,QAAA,CAAA,sBAAA,CAAA,WAAA,EAAA,CAAA,EAAA,KAAA,CAAA,OAAA,GAAA,EAFA;AAGA,OALA,MAKA;AACA,aAAA,eAAA;AACA;AACA,KAjBA;AAkBA,IAAA,OAlBA,mBAkBA,CAlBA,EAkBA;AAAA;;AACA,UAAA,KAAA,GAAA,CAAA,CAAA,MAAA,CAAA,KAAA,IAAA,CAAA,CAAA,YAAA,CAAA,KAAA;AACA,UAAA,CAAA,KAAA,CAAA,MAAA,EAAA;AACA,UAAA,MAAA,GAAA,IAAA,UAAA,EAAA;;AACA,MAAA,MAAA,CAAA,MAAA,GAAA,UAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,aAAA,GAAA,KAAA;;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,UAAA,CAAA,IAAA,CAAA;AACA,UAAA,GAAA,EAAA,CAAA,CAAA,MAAA,CAAA;AADA,SAAA,EAEA,IAFA,CAEA,QAAA,CAAA,sBAAA,CAAA,WAAA,EAAA,CAAA,EAAA,KAAA,CAAA,OAAA,GAAA,EAFA;AAIA,OANA;;AAQA,MAAA,MAAA,CAAA,aAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AAEA,KAhCA;AAiCA,IAAA,eAjCA,6BAiCA;AACA,UAAA,KAAA,GAAA,KAAA,QAAA,EAAA;AACA,eAAA,KAAA,KAAA,CAAA,UAAA,CAAA,IAAA,CAAA;AACA,UAAA,GAAA,EAAA;AADA,SAAA,CAAA;AAGA,OAJA,MAIA;AACA,eAAA,KAAA,KAAA,CAAA,UAAA,CAAA,IAAA,CAAA;AACA,UAAA,GAAA,EAAA;AADA,SAAA,CAAA;AAGA;AACA,KA3CA;AA4CA,IAAA,UA5CA,wBA4CA;AACA,WAAA,eAAA,GAAA,IAAA,CAAA,QAAA,CAAA,sBAAA,CAAA,WAAA,EAAA,CAAA,EAAA,KAAA,CAAA,OAAA,GAAA,MAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA,MAAA,CAAA,KAAA,GAAA,IAAA;AACA,KAhDA;AAiDA,IAAA,IAjDA,kBAiDA;AAAA;;AACA,UAAA,KAAA,aAAA,EAAA;AACA,YAAA,GAAA,GAAA,KAAA,GAAA;AACA,aAAA,WAAA,CAAA;AAAA,UAAA,GAAA,EAAA;AAAA,SAAA;AACA,OAHA,MAGA;AACA,YAAA,OAAA,GAAA;AACA,UAAA,IAAA,EAAA,QADA;AAEA,UAAA,IAAA,EAAA;AAAA,YAAA,KAAA,EAAA,GAAA;AAAA,YAAA,MAAA,EAAA;AAAA,WAFA;AAGA,UAAA,MAAA,EAAA;AAHA,SAAA;AAKA,aAAA,KAAA,CAAA,UAAA,CAAA,MAAA,CAAA,OAAA,EAAA,UAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,WAAA,CAAA;AAAA,YAAA,KAAA,EAAA;AAAA,WAAA;AACA,SAFA;AAGA;AAGA;AAjEA;AArBA,CAAA","sourcesContent":["<template>\n\n    <b-modal static @show=\"initModal\" id=\"photo-modal\" title=\"Profile image\" hide-footer >\n        <div v-if=\"alert.failed\" :class=\"`alert ${alert.type}`\">{{alert.message}}</div>\n        <form id=\"photo-popup\" @submit.prevent=\"crop\" class=\"cms\">\n\n                <vue-croppie ref=\"croppieRef\" :enableOrientation=\"false\" :showZoomer=\"false\"  :enableResize=\"false\" :mouseWheelZoom=\"false\"  :boundary=\"{ width: 230, height: 230}\" :viewport=\"{ width:200, height:200, 'type':'circle' }\">\n                </vue-croppie>\n\n            <div v-if=\"defaultAvatar\" class=\"avatar\">\n                <div class=\"form-check-inline\">\n                    <label class=\"form-check-label\"><input type=\"radio\" class=\"form-check-input\" @change=\"handleSexChange\"  v-model=\"sex\" value=\"Female\" name=\"gender\">Female</label>\n                </div>\n                <div class=\"form-check-inline\">\n                    <label class=\"form-check-label\"><input type=\"radio\" class=\"form-check-input\" @change=\"handleSexChange\"  v-model=\"sex\" value=\"Male\" name=\"gender\">Male</label>\n                </div>\n            </div>\n\n            <div class=\"photo-icons\">\n                <div class=\"form-group icon-container\">\n                    <label for=\"upload\" class=\"icon camera\"><font-awesome-icon icon=\"camera\" /></label>\n                    <input type=\"file\" class=\"form-control-file\" id=\"upload\" ref=\"upload\" @change=\"croppie\" >\n                    <p>Upload</p>\n                </div>\n                <div class=\"icon-container\">\n                    <a class=\"icon trash\" v-on:click=\"clearImage\">\n                        <font-awesome-icon icon=\"trash\" />\n                    </a>\n                    <p>Delete</p>\n                </div>\n            </div>\n            <div class=\"btn-container\">\n                <a class=\"btn btn-primary btn-border btn-cancel\" @click=\"$bvModal.hide('photo-modal')\">Cancel</a>\n                <button type=\"submit\"  class=\"btn btn-primary btn-border btn-save\" :disabled=\"account.status.uploadingImage\">Save\n                    <img v-show=\"account.status.uploadingImage\" src=\"data:image/gif;base64,\" />\n                </button>\n            </div>\n        </form>\n    </b-modal>\n\n</template>\n\n<script>\n    import {mapState, mapActions} from \"vuex\";\n\n    export default {\n        data() {\n            return {\n                defaultAvatar: true,\n                sex: null\n            };\n        },\n        props: ['profileProp'],\n        computed: {\n            ...mapState({\n                alert: state => state.alert,\n                account: state => state.account\n            }),\n\n        },\n        watch: {\n            'account.status': function (val) {\n                if(val.uploadedImage)\n                    this.$bvModal.hide('photo-modal');\n            }\n        },\n        methods: {\n            ...mapActions('account', ['uploadImage']),\n            ...mapActions('alert', ['clear']),\n            initModal() {\n                this.clear();\n                this.sex = this.profileProp.profile.sex;\n                if(!this.sex){\n                    this.sex = 'Female'\n                }\n                if(this.profileProp.profile.photo) {\n                    this.defaultAvatar = false;\n                    this.$refs.croppieRef.bind({\n                        url: this.profileProp.profile.photo\n                    }).then(document.getElementsByClassName('cr-slider')[0].style.display = '');\n                } else {\n                    this.handleSexChange();\n                }\n            },\n            croppie (e) {\n                var files = e.target.files || e.dataTransfer.files;\n                if (!files.length) return;\n                var reader = new FileReader();\n                reader.onload = e => {\n                    this.defaultAvatar = false;\n                    this.$refs.croppieRef.bind({\n                        url: e.target.result\n                    }).then(document.getElementsByClassName('cr-slider')[0].style.display = '');\n\n                };\n\n                reader.readAsDataURL(files[0]);\n\n            },\n            handleSexChange() {\n                if (this.sex === 'Female') {\n                   return this.$refs.croppieRef.bind({\n                        url: '/avatar_female.png',\n                    })\n                } else {\n                    return this.$refs.croppieRef.bind({\n                        url: '/avatar_male.png',\n                    })\n                }\n            },\n            clearImage () {\n                this.handleSexChange().then(document.getElementsByClassName('cr-slider')[0].style.display = 'none')\n                this.defaultAvatar = true;\n                this.$refs.upload.value=null;\n            },\n            crop() {\n                if(this.defaultAvatar) {\n                    let sex = this.sex;\n                    this.uploadImage({sex});\n                } else {\n                    let options = {\n                        type: 'base64',\n                        size: { width: 400, height: 400 },\n                        format: 'png'\n                    };\n                    this.$refs.croppieRef.result(options, image => {\n                        this.uploadImage({image});\n                    });\n                }\n\n\n            }\n        }\n    };\n</script>\n"],"sourceRoot":"src/components/profile/popups"}]}