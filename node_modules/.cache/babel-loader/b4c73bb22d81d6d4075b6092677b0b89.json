w VueRouter({\n    mode: 'history',\n    linkExactActiveClass: 'active',\n    scrollBehavior() {\n      return { x: 0, y: 0 };\n    },\n    routes: [\n      { path: '/', component: HomeProperties },\n      { path: '/dashboard', component: DashboardProperties },\n      { path: '/pricing', component: PricingProperties },\n      { path: '/register',  name: 'search', component: RegistrationProperties, props: true },\n      { path: '/login', component: LoginProperties },\n      { path: '/help', component: HelpProperties },\n      { path: '/account', component: AccountProperties },\n      { path: '/subscription-confirmed', component: OrderedProperties },\n\n      // otherwise redirect to home\n      { path: '*', redirect: '/' }\n    ]\n});\n\nrouter.beforeEach((to, from, next) => {\n    // redirect to login page if not logged in and trying to access a restricted page\n    const publicPages = [\"/\", '/login', '/pricing', '/register', '/login', '/subscription-confirmed'];\n    const publicPrefixes = [\"/tutor\" ];\n    const authRequired = !publicPages.includes(to.path) && !publicPrefixes.some(prefix => to.path.startsWith(prefix));\n    const loggedIn = localStorage.getItem('user');\n    if (authRequired && !loggedIn) {\n        return next('/login');\n    }\n\n\n    next();\n})\n"]}]}