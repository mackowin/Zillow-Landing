    <div class=\"hero-content\">\n                                <div class=\"row\">\n                                    <div class=\"col-lg-11\">\n                                        <h1>Find London properties with the <span style=\"color: #5457c1\">highest return</span></h1>\n                                        <h5>Put your search on autopilot</h5>\n\n                                        <div class=\"tick-section\">\n                                            <div class=\"d-block d-sm-none\">\n                                                <div class=\"tick-row\">\n                                                    <font-awesome-icon class=\"fa-2x tick-icon\" icon=\"check-square\" />\n                                                    <p class=\"tick-input\">Find properties with the highest return potential</p>\n                                                </div>\n                                            </div>\n\n                                                <div class=\"d-none d-sm-block\">\n                                                    <div class=\"tick-row\">\n                                                        <font-awesome-icon class=\"fa-2x tick-icon\" icon=\"check-square\" />\n                                                        <p class=\"tick-input\">Find properties with the highest return potential</p>\n                                                    </div>\n                                                </div>\n\n                                            <div class=\"d-block d-sm-none\">\n                                                <div class=\"tick-row\">\n                                                    <font-awesome-icon class=\"fa-2x tick-icon\" icon=\"check-square\" />\n                                                    <p class=\"tick-input\">Analyze rent-to-price, yield, and other ROI metrics</p>\n                                                </div>\n                                            </div>\n\n                                                <div class=\"d-none d-sm-block\">\n                                                    <div class=\"tick-row\">\n                                                        <font-awesome-icon class=\"fa-2x tick-icon\" icon=\"check-square\" />\n                                                        <p class=\"tick-input\">Analyze rent-to-price, yield, and other ROI metrics</p>\n                                                    </div>\n                                                </div>\n\n                                            <div class=\"d-block d-sm-none\">\n                                                <div class=\"tick-row\">\n                                                    <font-awesome-icon class=\"fa-2x tick-icon\" icon=\"check-square\" />\n                                                    <p class=\"tick-input\">Outrun other bidders, find hidden gems</p>\n                                                </div>\n                                            </div>\n\n                                                <div class=\"d-none d-sm-block\">\n                                                    <div class=\"tick-row\">\n                                                        <font-awesome-icon class=\"fa-2x tick-icon\" icon=\"check-square\" />\n                                                        <p class=\"tick-input\">Outrun other bidders, find hidden gems</p>\n                                                    </div>\n                                                </div>\n                                        </div>\n\n                                        <div style=\"margin-top: 5rem\">\n                                            <div v-if=\"show_contact && contact_notice != ''\" class=\"alert alert-warning\">\n                                                There was a problem submitting your email. {{ contact_notice }}\n                                            </div>\n\n                                            <form v-if=\"show_contact\" id=\"sign\" @submit.prevent=\"addEmail(email)\" class=\"mt-4\">\n                                                <div class=\"row\">\n                                                    <div class=\"col-sm-7\" style=\"margin-top: 5px\">\n                                                        <input type=\"email\"  name=\"properties\" id=\"properties-lead\" class=\"form-control\"  v-model=\"email\" required data-error=\"Please enter your email\" placeholder=\"Enter your email address\">\n                                                        <p class=\"tick-input\" style=\"font-size: 10px; color:#495057\">Sign up to review properties</p>\n                                                    </div>\n                                                    <div class=\"col-sm-5\" style=\"margin-top: 5px\">\n                                                        <button type=\"submit\" @click=\"goToSearch()\" class=\"btn btn-primary\">START</button>\n                                                    </div>\n                                                </div>\n                                            </form>\n                                        </div>\n\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n\n\n                        <div class=\"col-xl-4 col-lg-3\" style=\"margin-top: 2rem\">\n                            <img src=\"../resources/banner-photo7.jpeg\" style=\"border-radius: 5%\">\n                        </div>\n                    </div>\n\n                </div>\n            </div>\n        </div>\n\n        <div class=\"shape1\"><img src=\"../resources/shape1.png\" ></div>\n        <div class=\"shape3\"><img src=\"../resources/shape3.svg\" ></div>\n        <div class=\"shape4\"><img src=\"../resources/shape4.svg\" ></div>\n        <div class=\"shape5\"><img src=\"../resources/shape5.png\"></div>\n        <div class=\"shape6 rotateme\"><img src=\"../resources/shape4.svg\" ></div>\n        <div class=\"shape7\"><img src=\"../resources/shape4.svg\" ></div>\n        <div class=\"shape8 rotateme\"><img src=\"../resources/shape2.svg\" ></div>\n    </div>\n    <!-- End Main Banner -->\n</template>\n\n\n\n<script>\n\n    export default {\n        name: 'BannerProperties',\n        data() {\n            return {\n                email: '',\n                show_contact: true,\n                contact_notice: ''\n            }\n        },\n        methods: {\n            async addEmail() {\n                if(!this.validateEmail(this.email)) {\n                    this.contact_notice = \"The email address is badly formatted.\";\n                } else {\n                    const url = `/api/contact/apply`;\n                    const requestOptions = {\n                        method: \"POST\",\n                        headers: {\"Content-Type\" : \"application/json\"},\n                        body: JSON.stringify({ email: this.email, idea: 'properties' })\n                    };\n                    fetch(url, requestOptions).then(this.show_contact = false)\n                }\n            },\n            validateEmail(email) {\n                var re = /\\S+@\\S+\\.\\S+/;\n                return re.test(String(email).toLowerCase());\n            },\n            goToSearch() {\n                if(this.validateEmail(this.email)) {\n                    this.$router.push({ name: 'search', params: { email: this.email } });\n                }\n            }\n        }\n    }\n\n</script>\n"]}]}