required</div>\n                        </div>\n                        <div class=\"form-group\">\n                            <label htmlFor=\"password\">Password</label>\n                            <input type=\"password\" v-model=\"password\" name=\"password\" class=\"form-control\" :class=\"{ 'is-invalid': submitted && !password }\" />\n                            <div v-show=\"submitted && !password\" class=\"invalid-feedback\">Password is required</div>\n                        </div>\n                        <router-link to=\"/password-reset\" >Forgot password?</router-link>\n                        <div class=\"btn-container\">\n                            <router-link to=\"/register\" class=\"btn btn-primary btn-border btn-cancel\">Register</router-link>\n                            <button class=\"btn btn-primary btn-border btn-save\" :disabled=\"status.loggingIn\">Login\n                                <img v-show=\"status.loggingIn\" src=\"data:image/gif;base64,\" />\n                            </button>\n                        </div>\n                    </form>\n                </div>\n            </div>\n        </div>\n    </div>\n\n</template>\n\n<script>\n    import { mapState, mapActions } from 'vuex'\n\n    export default {\n        data () {\n            return {\n                email: '',\n                password: '',\n                submitted: false\n            }\n        },\n        computed: {\n            ...mapState('account', ['status'])\n        },\n        created () {\n            // reset login status\n            this.logout();\n        },\n        methods: {\n            ...mapActions('account', ['login', 'logout']),\n            handleSubmit () {\n                this.submitted = true;\n                const { email, password } = this;\n                if (email && password) {\n                    this.login({ email, password })\n                }\n            }\n        }\n    };\n</script>\n"]}]}